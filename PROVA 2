velocidade = float(input("Digite a velocidade do carro em km/h:
-> "))

if velocidade > 80:
    excesso = velocidade - 80
    multa = excesso * 20.00
    print(f"Você superou o limite da via e foi multado!")
    print(f"Sua divida é de: R${multa:.2f}.")
else:
    print("Velocidade dentro do limite.")
